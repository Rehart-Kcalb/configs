{
    "languageserver": {
      "golang": {
        "command": "gopls",
        "rootPatterns": ["go.mod"],
        "filetypes": ["go"]
      }
    },
    "markdownlint.config": {
        "default": true,
        "line-length": true
    },
    "markdownlint.onSave": true,
    "sql.database": "postgresql",
    "sql.formatOptions": {
        "language": "postgresql",
        "keywordCase": "upper",
        "dataTypeCase": "upper"
    }
}
